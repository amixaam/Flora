{
    "expo": {
        "scheme": "flora",
        "name": "Flora",
        "slug": "Flora",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#050506"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.Flora"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#050506"
            },
            "package": "com.anonymous.Flora"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [["expo-font"], "expo-router"],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "39abf8de-c6dc-4b0b-b412-239f936d7927"
            }
        }
    }
}
